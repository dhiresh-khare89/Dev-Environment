<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Converted_to_new_Client_Website</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(Status , &quot;Converted to New Client&quot;),
