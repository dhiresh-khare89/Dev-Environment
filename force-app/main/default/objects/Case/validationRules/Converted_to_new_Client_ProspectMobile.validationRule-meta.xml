<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Converted_to_new_Client_ProspectMobile</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(Status , &quot;Converted to New Client&quot;),
ISCHANGED(Status))
&amp;&amp;
 ISBLANK(SuppliedPhone)</errorConditionFormula>
    <errorDisplayField>SuppliedPhone</errorDisplayField>
    <errorMessage>Please fill the Prospect Mobile.</errorMessage>
</ValidationRule>
