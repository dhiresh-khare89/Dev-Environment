<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Mobile_Validation_Accept_Numeric_Value</fullName>
    <active>false</active>
    <errorConditionFormula>AND
(
	NOT(ISBLANK(SuppliedPhone)),
	NOT(ISNUMBER(SuppliedPhone))
)</errorConditionFormula>
    <errorDisplayField>SuppliedPhone</errorDisplayField>
    <errorMessage>Please insert only the numeric value</errorMessage>
</ValidationRule>
