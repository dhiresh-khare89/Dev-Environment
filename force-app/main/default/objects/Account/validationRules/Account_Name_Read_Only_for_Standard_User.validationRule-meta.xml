<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Name_Read_Only_for_Standard_User</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISCHANGED(Name),$Profile.Name&lt;&gt;&apos;System Administrator&apos;)</errorConditionFormula>
    <errorMessage>Account Name is Not Editable</errorMessage>
</ValidationRule>
