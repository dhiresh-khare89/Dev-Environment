<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Tentative_Fee_not_be_able_to_change_it</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(PRIORVALUE(  TentativeFee__c  ))), ISCHANGED(  TentativeFee__c  ))</errorConditionFormula>
